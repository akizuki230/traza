<%= simple_form_for(@content) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :contenido %>
    <%= f.input :descripcion %>
    <%= f.input :creator_id %>
    <%= f.input :category_id %>
    <%= f.input :type_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
